/* to find and display the multiplication table */

#include<stdio.h>

void main()
	{
		int num, i=1,p;

		printf("\n Enter the number: ");
		scanf("%d", &num);

		printf("\n Multiplication Table of %d : ", num);

		while(i<=10)
			{
                                p=num*i;
				printf("\n %d x %d = %d ",num,i,p);
				i++;
			}

	}
